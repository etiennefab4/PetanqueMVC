@model Petanque.Web.Models.TeamDto

@{
    Layout = "../Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
    <head>
        <title>Edit Team</title>
    </head>
    <body>
        <div>
              @using (Html.BeginForm("Edit", "Team","POST"))
        {
            <fieldset>
                @Html.DisplayFor(x => x.Nom)
                @Html.TextBoxFor(x => x.Nom)
            </fieldset>
             <p>
               <input type="submit" value="Save" />
           </p>
        }
        </div>
    </body>
</html>