@model Petanque.Web.Models.CompetitionDto
@{
    Layout = "../Shared/_Layout.cshtml";
    ViewBag.Title = "Créer competition";
}
<div>
    @using (Html.BeginForm("Create", "Competition", "POST"))
    {
        <fieldset>
            @Html.DisplayFor(x => x.Nom)
            @Html.TextBoxFor(x => x.Nom)
        </fieldset>
        <p>
            <input type="submit" value="Save" />
        </p>
    }
</div>
